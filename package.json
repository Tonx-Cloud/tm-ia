{
  "name": "ai-music-video-api",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npx tsx dev-server.ts",
    "dev:api": "npx tsx dev-server.ts",
    "build:api": "npx tsc",
    "build:web": "npm --prefix web run build",
    "worker": "npx tsx worker.ts",
    "build": "npm run build:api",
    "build:vercel": "npx prisma migrate deploy && npx prisma generate && npm run build:api && npm run build:web",
    "lint": "eslint \"api/**/*.ts\" \"dev-server.ts\"",
    "lint:fix": "eslint \"api/**/*.ts\" \"dev-server.ts\" --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "@google/generative-ai": "^0.19.0",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.93.3",
    "@vercel/blob": "^2.0.1",
    "@vercel/node": "^5.5.28",
    "ai": "^5.0.124",
    "busboy": "^1.6.0",
    "dotenv": "^17.2.3",
    "ffmpeg-static": "^5.3.0",
    "formidable": "^3.5.4",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.3",
    "mercadopago": "^2.12.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.83.0",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.32",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "tsx": "^4.19.4",
    "typescript": "^5.9.3"
  }
}
